{
  "version": 2,
  "builds": [
    {
      "src": "api/webserver.py", 
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"   
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "api/webserver.py" 
    }
  ]
}
